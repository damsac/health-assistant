{
  "name": "ai-health-consultant-3",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start",
    "dev:web": "npx expo export -p web && npx serve dist/client",
    "export:web": "npx expo export -p web",
    "serve:web": "npx serve dist/client",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "agent": "bun run server/index.ts",
    "agent:dev": "bun --watch run server/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/react": "^3.0.5",
    "@hono/node-server": "^1.19.7",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/native": "^7.1.8",
    "@tamagui/config": "^1.121.0",
    "@tamagui/font-inter": "^1.121.0",
    "@tanstack/react-query": "^5.90.16",
    "ai": "^6.0.5",
    "better-auth": "^1.2.8",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.3",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "garmin-connect": "^1.6.2",
    "hono": "^4.11.3",
    "postgres": "^3.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.69.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.121.0",
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/react": "~19.1.0",
    "drizzle-kit": "^0.31.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "supabase": "^2.23.4",
    "typescript": "~5.9.2"
  },
  "engines": {
    "bun": "1.3.5",
    "node": "24.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "prepare": "off"
}
